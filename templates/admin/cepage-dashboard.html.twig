{% extends 'base.html.twig' %}

{% block title %}Admin | Cepage Management {% endblock %}

{% block meta %}
	<meta name="description" content="Enrichissez notre collection de champagnes en ajoutant de nouveaux cépages ! Gérez facilement les informations des variétés de raisin en incluant les noms et contribuez ainsi à la diversité de nos vins artisanaux, élaborés avec passion et tradition.">
	<meta name="author" content="David Pires">
{% endblock %}

{% block body %}
<div class="home">
<h1 class="mb-5 liNav text-center">Insertion Cépage</h1>
	<div class="container mb-5 w-50 mt-4">
		<div class="row liNav">
				{{ form_start(CepageForm) }}
				{{ form_row(CepageForm.name) }}

				<button type="submit" class="btn btn-outline-success mb-5 mt-4">Valider</button>
				{{ form_end(CepageForm) }}
		</div>
	</div>
</div>

{% endblock %}